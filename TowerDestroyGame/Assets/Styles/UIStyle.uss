.progressBar .unity-progress-bar__background {
    background-image: url('project://database/Assets/Sprites/1x/Icons/health_bar_icon.png?fileID=21300000&guid=d33f3037a8854bc4bb1d75284b007f94&type=3#health_bar_icon');
    background-color: rgba(255, 255, 255, 0);
    border-left-color: rgba(255, 255, 255, 0);
    border-right-color: rgba(255, 255, 255, 0);
    border-top-color: rgba(255, 255, 255, 0);
    border-bottom-color: rgba(255, 255, 255, 0);
    -unity-background-scale-mode: scale-to-fit;
    color: rgba(255, 255, 255, 0);
